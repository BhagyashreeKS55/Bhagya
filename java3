import java.util.Scanner;
public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N,i,z;
        N = sc.nextInt();
            for(i=1;i<=10;i++){
               z=N*i;
            System.out.println(N+" x "+i+" = "+z); 
            }
    }
}
